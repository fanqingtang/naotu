    "css-loader": "^1.0.1",
    "cz-conventional-changelog": "^1.1.5",
    "element-ui": "^2.4.9",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "grunt": "~0.4.1",
    "grunt-angular-templates": "~0.5.0",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^0.5.0",
    "grunt-contrib-concat": "~0.5.0",
